{
	"type": "project",
	"license": "AGPLv3",
	"require": {
		"php": ">=5.6.0",
		"ext-ctype": "*",
		"ext-iconv": "*",
		"symfony/console": "3.4.12",
		"symfony/dotenv": "3.4.12",
		"symfony/framework-bundle": "3.4.12",
		"symfony/twig-bundle": "3.4.12",
		"symfony/yaml": "3.4.12",
		"symfony/http-foundation": "3.4.12"
	},
	"config": {
		"vendor-dir": "../../../../lib/composer-vendor",
		"preferred-install": {
			"*": "dist"
		},
		"sort-packages": true,
		"platform": {
			"php": "5.6.0"
		}
	},
	"autoload": {
		"psr-4": {
			"Combodo\\iTop\\Portal\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Combodo\\iTop\\Portal\\Tests\\": "tests/"
		}
	},
	"replace": {
		"paragonie/random_compat": "2.*",
		"symfony/polyfill-ctype": "*",
		"symfony/polyfill-iconv": "*",
		"symfony/polyfill-php70": "*",
		"symfony/polyfill-php56": "*"
	},
	"conflict": {
		"symfony/symfony": "*"
	},
	"extra": {
		"symfony": {
			"allow-contrib": false,
			"require": "3.4.*"
		}
	}
}
